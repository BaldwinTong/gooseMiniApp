/* pages/memberStyle/memberStyle.wxss */
.page{
  width: 100%;
}

.article{
  width: 100%;
  padding: 20rpx 32rpx;
  box-sizing: border-box;
}

.article .title{
  color: #000;
  font-size: 28rpx;
  font-weight: 600;
  letter-spacing:2rpx;
  margin: 20rpx 0;
}

.article .content{
  color: #696969;
  font-size: 28rpx;
  letter-spacing:4rpx;
}

.article image{
  width: 100%;
  margin: 20rpx 0;
}
