/* pages/personalRegister/personalRegister.wxss */
.page{
  width: 100%;
  padding: 30rpx 36rpx;
  box-sizing: border-box;
}

.item:first-child{
  padding-top: 0 ;
}

.item{
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid rgba(249,249,250, 1);
  position: relative;
  padding: 28rpx 0;
  box-sizing: border-box;
}

.item .title{
  font-size: 30rpx;
  font-weight: 500;
  color: #000;
  letter-spacing: 1px;
}

.placeH{
  text-align: right;
  color: #B9B9B9;
}

.scan{
  width: 40rpx;
  height: 40rpx;
  margin-left: 24rpx;
}

.item .itemCont{
  display: flex;
  justify-content: center;
  align-items: center;
}

.item .title_ps{
  color: #B9B9B9;
  font-size: 24rpx;
  margin-top: 10rpx;
}

.item .IDimage{
  width: 286rpx;
  height: 180rpx;
}

.item2 .title{
  height: 180rpx;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: flex-start;
}

.line{
  width: 100%;
  height: 28rpx;
  background-color: #F1F2F5;
}

.itemCont .getCode{
  width: 176rpx;
  height: 64rpx;
  border-radius: 64rpx;
  background-color: #0083FF;
  text-align: center;
  line-height: 64rpx;
  color: #fff;
  font-size: 24rpx;
  margin-left: 12rpx;
}

.item .tokenCode{
  background-color: #ccc;
}

.btn{
  width: 100%;
  height: 100rpx;
  background-color: #0083FF;
  text-align: center;
  line-height: 100rpx;
  color: #fff;
  font-size: 32rpx;
  letter-spacing: 16rpx;
  margin-top: 60rpx;
  border-radius: 8rpx;
}
